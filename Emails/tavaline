      <!-- sponsor logo --> 
      <tr> 
       <td style="padding-bottom:15px; min-width:100%; width:100%;"> 
        <table width="100%" cellpadding="0" cellspacing="0" border="0" style="background-color:#FFFFFF; min-width:100%;" role="presentation"> 
         <tbody> 
          <tr> 
           <td align="center" style="min-width:100%; width: 100%;"> 
            <table cellspacing="0" cellpadding="0" border="0"> 
             <tbody>
              <tr> 
               <td width="50%" valign="middle" style="text-align:right;padding-right:6px;"><p style="font-family:Helvetica,sans-serif;font-size:12px;color:#6E7381;margin:0;">Presented by</p></td> 
               <td width="50%" valign="middle" style="text-align:left;"> 
                <div style="width:100%;max-width:90px;"> 
                 <img width="90" src="https://static.politico.com/01/8c/93d7696340d5a282fe95863e89ef/wf-thrd-prty-1line-rgb-red-f1.jpg" alt="Wells Fargo" border="0" style="width:90px;max-width:100%;" /> 
                </div> </td> 
